
<%- contentFor('hero') %>


<%- contentFor('body') %>
<div class="jumbotron text-center">
  <h1>The Team</h1>

  <div class="row">
    <% for (user of users) { %>
      <div class="col-sm-3">
        <h2><%= user.name %>
          <img src="<%= user.avatar %>" alt="" class="img-responsive">
          <h5><%= user.email %></h5>
      </div>
    <% } %>
  </div>
</div>
